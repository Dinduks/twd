      "files": ["node-webkit (contained in prebuilt binaries)"]
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/passcod/twd.git"
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-node-webkit-builder": "~0.1.3",
    "grunt-shell": "~0.3.1"
  },
  "engines": {
    "node-webkit": ">=0.7.0"
  },
  "main": "app://twd/index.html",
  "nodejs": true,
  "single-instance": true,
  "user-agent": "Mozilla/5.0 (%osinfo) WebKit/%webkit_ver NodeWebkit/%nwver twd/%version",
  "window": {
    "icon": "icon.png",
    "toolbar": false,
    "height": 700,
    "width": 900,
    "min_width": 430,
    "title": "TweetDeck",
    "position": "centre",
    "resizable": true
